<div class="row">
  <div class="col-md-12">
    <%= form_for @wiki do |f| %>
    <div class="form-group">
      <h1><%= f.text_field :title, placeholder: "Name your Wiki", :autofocus => true %></h1>
      <p><%= f.text_area :body, :size => "75x10" %></p>
      <p><%= f.label :private %> <%= f.check_box :private %></p>
    <%= f.submit "Save" , class: 'btn btn-primary' %>
    <% end %>
    <%= link_to "Cancel", :back, class: 'btn btn-danger' %>
  </div>
</div>